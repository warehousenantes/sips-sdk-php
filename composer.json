{
  "name": "worldline/sips-payment-sdk",
  "description": "This library simplifies the use of Sips 2.0, the e-payments API.",
  "keywords": [
    "Sips",
    "Worldline",
    "PSP",
    "Payments",
    "Bancontact"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Ros",
      "email": "supportsips-benelux@worldline.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "nyholm/psr7": "^1.8",
    "php-http/discovery": "^1.0",
    "php-http/httplug": "^2.0",
    "php-http/message-factory": "^1.0",
    "psr/http-client-implementation": "^1.0",
    "psr/http-message": "^2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "php-http/message": "^1.0",
    "php-http/mock-client": "^1.0",
    "phpstan/phpstan": "^1.5",
    "phpunit/phpunit": "^9.5.0",
    "rector/rector": "^0.12.20",
    "symfony/http-client": "^7.0"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "autoload": {
    "psr-4": {
      "Worldline\\Sips\\": "src"
    },
    "files": [
      "src/Common/Values.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Worldline\\Sips\\Test\\": "tests"
    }
  }
}
