{
  "name": "worldline/sips-payment-sdk",
  "description": "This library simplifies the use of Sips 2.0, the e-payments API.",
  "keywords": [
    "Sips",
    "Worldline",
    "PSP",
    "Payments",
    "Bancontact"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Ros",
      "email": "supportsips-benelux@worldline.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "symfony/http-client": "^7.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "phpstan/phpstan": "^1.5",
    "phpunit/phpunit": "^9.5.0",
    "rector/rector": "^0.12.20"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Worldline\\Sips\\": "src"
    },
    "files": [
      "src/Common/Values.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Worldline\\Sips\\Test\\": "tests"
    }
  }
}
